{
    "name": "mi32dogs/silverstripe-shortcodable",
    "description": "Provides a GUI for CMS users to insert Shortcodes into the HTMLEditorField + an API for developers to define Shortcodable DataObjects and Views",
    "type": "silverstripe-vendormodule",
    "keywords": [],
    "license": "BSD-3-Clause",
    "authors": [
        {"name": "Rob van Tuin","email": "mi32dogs@gmail.com"}
    ],
    "require": {
        "composer/installers": "*",
        "silverstripe/admin": "^1",
        "silverstripe/framework": "^4"
    },
    "autoload": {
        "psr-4": {
            "SilverStripe\\Shortcodable\\": "src/"
        }
    },
    "support": {
        "issues": "https://github.com/mi32dogs/silverstripe-shortcodable/issues"
    },
    "extra": {
        "expose": [
            "css",
            "images",
            "javascript"
        ]
    },
    "homepage": "https://github.com/mi32dogs/silverstripe-shortcodable"
}
